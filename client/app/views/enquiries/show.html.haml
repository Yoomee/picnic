- title "#{@enquiry.form_name} Form Enquiry"

-@enquiry.enquiry_fields.each do |field|
  .fieldName
    ="#{field.name.humanize}:"
  .fieldValue
    =field.value
.fieldName
  Sent on:
.fieldValue
  =@enquiry.created_at
%br
  
  
-options_panel do
  =back_link
  = link_to "Delete", @enquiry, :confirm => 'Are you sure?', :method => :delete
  = link_to "View All", enquiries_path
